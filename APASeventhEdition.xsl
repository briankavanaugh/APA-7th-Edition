													<xsl:variable name="noCommaBeforeAnd">
														<xsl:call-template name="templ_prop_NoCommaBeforeAnd" />
													</xsl:variable>

													<xsl:variable name="noAndBeforeLastAuthor">
														<xsl:call-template name="templ_prop_NoAndBeforeLastAuthor"/>
